{
    "name": "meklis/snmp-wrapper",
    "description": "PHP wrapper for working with helpprovider SnmpWrapper",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Boyar",
            "email": "max.boyar.a@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SnmpWrapper\\": "src/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "7.5.x-dev",
        "karriere/json-decoder": "^2.2"
    },
    "scripts": {
        "post-update-cmd": [
            "php -r \"exec('chmod +x ' . __DIR__  . '/vendor/meklis/snmp-wrapper/src/SnmpT/snmpt');\""
        ],
        "post-install-cmd": [
            "php -r \"exec('chmod +x ' . __DIR__  . '/vendor/meklis/snmp-wrapper/src/SnmpT/snmpt');\""
        ]
    },
    "minimum-stability": "dev"
}
